    <property name="styleSheet">
     <string notr="true">QProgressBar#chargeBar {
    background-color: #E5E7EB;  /* Açık gri-beyaz arka plan */
    border: 2px solid #D1D5DB;  /* Hafif gri çerçeve */
    border-radius: 10px;  /* Yuvarlak kenarlar */
    height: 20px;  /* Yükseklik */
    text-align: center;  /* Metni ortala */
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.3);  /* Hafif gölge efekti */
}

QProgressBar#chargeBar::chunk {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
        stop:0  #676d42, stop:1  #676d42);  /* Beyazdan açık griye gradyan (dolu kısım) */
    border-radius: 8px;  /* İç çubuğun yuvarlak kenarları */
    border: 1px solid #FFFFFF;  /* Beyaz kenarlık */
}

QProgressBar#chargeBar::text {
    color: #000000 !important;  /* Metni siyah yap */
    font-weight: bold;  /* Kalın yazı tipi */
    font-size: 12px;  /* Yazı boyutu */
    text-shadow: 1px 1px 2px rgba(255, 255, 255, 0.5);  /* Hafif beyaz gölge efekti */
}</string>
